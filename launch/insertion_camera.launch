<?xml version="1.0"?>
<launch>



<!-- respawn="true" -->
    <!-- set exposure to -1 (or delete parameter to activate auto exposure -->
      <!-- get(device_name='MARU_0003_insert').data -->
<!-- frame to which the camera is attached -->
  <node name="pylon" pkg="pylon_camera" type="pylon_camera_node" output="log"  respawn_delay="5" respawn="true" >
      <param name="camera_frame" value="reference_camera_base" type="string" />
      
      <param name="exposure" value="22001" type="int" />
      <param name="desired_framerate" value="5" type="double" />
      <param name="camera_frame" value="reference_camera_base" type="string" /> 

      <param name="magazino_cam_id"  value="MARU_0003_insert"  type="string" />
      
      <param name="intrinsic_yaml_string" command="python -c 'import django; django.setup(); from maru_status.models import IntrinsicCalibration; print IntrinsicCalibration.objects.get(CamID=22).data'" />

  </node>



</launch>




