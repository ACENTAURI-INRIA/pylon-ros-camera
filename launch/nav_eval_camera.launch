<?xml version="1.0"?>
<launch>
 <node pkg="pylon_camera" name="nav_eval_camera" type="pylon_camera_node"> <!--  respawn="true" respawn_delay="30" -->
  <param name="camera_frame" value="/nav_eval_camera" type="string" /> <!-- frame to which the camera is attached -->
  <param name="exposure" value="-1" type="int" />
  <param name="use_sequencer" value="false" type="bool" />
  <param name="desired_framerate" value="10.0" type="double" />
  <param name="use_brightness" value="true" type="bool" />
  <param name="intrinsic_yaml_string" command="cat $(find pylon_camera)/calib/nav_eval_dart_cam.yml"/>
  </node>
</launch>
