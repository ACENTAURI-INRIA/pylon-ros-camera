# set_demosaicing_mode :
	0 = Simple
	1 = Basler PGI

# set_light_source_preset :
	0 = Off
	1 = Daylight5000K
	2 = Daylight6500K
	3 = Tungsten2800K

# set_balance_white_auto :
	0 = Off
	1 = Once
	2 = Continuous

# set_sensor_readout_mode :
	0 = Normal  
	1 = Fast

# set_trigger_selector :
	0 = Frame start
	1 = Frame burst start (ace USB cameras) / Acquisition Start (ace GigE cameras)

# set_trigger_source :
	0 = Software
	1 = Line1
	2 = Line3
	3 = Line4
	4 = Action1 (only selected GigE Camera)

# set_trigger_activation :
	0 = RigingEdge
	1 = FallingEdge

# set_line_selector :
	0 = Line1
	1 = Line2
	2 = Line3
	3 = Line4

# set_line_mode :
	0 = Input
	1 = Output

# set_line_source :
	0 = Exposure Active
	1 = FrameTriggerWait
	2 = UserOutput1
	3 = Timer1Active
	4 = FlashWindow(only with rolling shutter)

# select_user_set :
	0 = Default
	1 = UserSet1
	2 = UserSet2
	3 = UserSet3
	4 = HighGain
	5 = AutoFunctions
	6 = ColorRaw

# select_default_user_set :
	0 = Default
	1 = UserSet1
	2 = UserSet2
	3 = UserSet3
	4 = HighGain
	5 = AutoFunctions
	6 = ColorRaw


