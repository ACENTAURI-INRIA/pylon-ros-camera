<launch>

  <!--node name="exp_comp_node" pkg="pylon_camera" type="compatibility_exposure_action.py" output="screen" respawn="true" respawn_delay="5" /-->
  <node name="crane" pkg="pylon_camera" type="pylon_camera_node" output="screen" respawn="true" respawn_delay="5" > 
    <!-- set exposure to -1 (or delete parameter to activate auto exposure -->
      <param name="pylon_exposure_mu_s" value="3000" type="int" />
      <param name="camera_identifier" value="192.168.112.4:3956" type="string" />
      <param name="camera_frame" value="crane_camera" type="string" /> <!-- frame to which the camera is attached -->
      <param name="intrinsic_cam_id" value="31" type="int" />  <!-- id to find intrinsic calib in DB -->
      <param name="write_exp_to_db" value="true" type="bool" />
      <param name="magazino_cam_id"  value="MARU_0003_crane"  type="string" />
     <param name="intrinsic_yaml_string" command="python -c 'import django; django.setup(); from maru_status.models import IntrinsicCalibration; print IntrinsicCalibration.objects.get(CamID=31).data'" />
    </node>
</launch>






