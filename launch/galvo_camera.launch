<?xml version="1.0"?>
<launch>
 <node pkg="pylon_camera" name="dart1" type="pylon_camera_node" output="screen"> <!--  respawn="true" respawn_delay="30" -->
  <param name="camera_frame" value="dart1" type="string" /> <!-- frame to which the camera is attached -->
  <param name="exposure" value="-1" type="int" />
  <param name="use_sequencer" value="true" type="bool" />
  <param name="desired_framerate" value="100.0" type="double" />
  <param name="use_brightness" value="true" type="bool" />
  <param name="magazino_cam_id"  value="dart_0001_x"  type="string" />
  <param name="intrinsic_yaml_string" command="cat $(find pylon_camera)/calib/dart_0001_x_intrinsic_calib.yml"/>

  <param name="use_sequencer" value="false" type="bool" />
  <param name="output_hdr_img" value="false" type="bool" />
  <param name="desired_framerate" value="15.0" type="double" />

 
 </node>
</launch>
