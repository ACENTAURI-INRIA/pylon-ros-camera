<launch>


  <node name="crane2" pkg="pylon_camera" type="pylon_camera_node" output="screen" respawn="true" respawn_delay="5" > 
    <!-- set exposure to -1 (or delete parameter to activate auto exposure -->
      <param name="pylon_exposure_mu_s" value="3000" type="int" />
      <param name="camera_identifier" value="Basler acA1300-60gm#0030531785F2#192.168.112.30:3956" type="string" />
      <param name="camera_frame" value="crane_camera" type="string" /> <!-- frame to which the camera is attached -->
      <param name="intrinsic_cam_id" value="14" type="int" />  <!-- id to find intrinsic calib in DB -->
      <param name="write_exp_to_db" value="true" type="bool" />
    </node>
</launch>






